<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jake.colorjumper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Jake's Game"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jake.colorjumper.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.jake.colorjumper.SettingsActivity"></activity>
        <activity android:name="com.jake.colorjumper.GameActivity"></activity>
        <activity android:name="com.jake.colorjumper.LoseActivity"></activity>
    </application>

</manifest>